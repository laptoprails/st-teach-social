<%= form_for [objectiveable, objectiveable.objectives.build], remote: true do |objective| %>
	
	<%= objective.text_field :objective, placeholder: "What skills will students learn?", class: 'span12' %>
	<%= objective.hidden_field :objective_type, value:"Skill" %>
	<%= objective.submit "Add", class:"btn btn-monkey pull-right" %>

<% end %>